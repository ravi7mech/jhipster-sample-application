        <addForeignKeyConstraint baseColumnNames="individual_id"
                                 baseTableName="cust_rel_party"
                                 constraintName="fk_cust_rel_party__individual_id"
                                 referencedColumnNames="id"
                                 referencedTableName="individual"/>
    </changeSet>
</databaseChangeLog>
